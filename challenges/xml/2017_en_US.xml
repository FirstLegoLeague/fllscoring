<fll:challenge
    xmlns:fll="http://fll-tools.com/applications/scoring/v1/challenge.xsd" year="2017" version="1" name="HYDRO DYNAMICS">

    <strings language="en-US">

        <string id="yes">                           Yes</string>
        <string id="no">                            No</string>

        <string id="M01-name">                      M01 Pipe Removal</string>
        <string id="M01-desc">                      <![CDATA[Move the Broken Pipe so it is completely in Base.]]></string>
        <string id="M01-scoring">                   Broken Pipe is completely in Base</string>
                   
        <string id="M02-name">                      M02 Flow</string>
        <string id="M02-desc">                      <![CDATA[Move a Big Water (one time maximum) to the other team’s field *only by turning the Pump System’s valve(s).]]></string>
        <string id="M02-scoring">                   Big Water is on other team's Field (only by turning Pump System's valves(s))</string>
                   
        <string id="M03-name">                      M03 Pump Addition</string>
        <string id="M03-desc">                      <![CDATA[Move the Pump Addition so it has contact with the mat and that contact is completely in the Pump Addition target.]]></string>
        <string id="M03-scoring">                   Pump Addition has contact with the mat completely inside the target area</string>

        <string id="M04-name">                      M04 Rain</string>
        <string id="M04-desc">                      <![CDATA[Make at least one Rain come out of the Rain Cloud.]]></string>
        <string id="M04-scoring">                   At least one Rain is out of the Rain Cloud</string>

        <string id="M05-name">                      M05 Filter</string>
        <string id="M05-desc">                      <![CDATA[Move the Filter north until the lock latch drops.]]></string>
        <string id="M05-scoring">                   Lock latch is in dropped position</string>
        
        <string id="M06-name">                      M06 Water Treatment</string>
        <string id="M06-desc">                      <![CDATA[Make the Water Treatment model eject its Big Water, *only by moving the Toilet’s lever.]]></string>
        <string id="M06-scoring">                   Big Water is ejected from Water Treatment model (only by Toilet's lever)</string>
        
        <string id="M07-name">                      M07 Fountain</string>
        <string id="M07-desc">                      <![CDATA[Make the Fountain’s middle layer rise some obvious height and stay there, due only to a Big Water in the gray tub.]]></string>
        <string id="M07-scoring">                   Middle layer is raised (due only to a Big Water in gray tub)</string>
        
        <string id="M08-name">                      M08 Manhole Covers</string>
        <string id="M08-desc">                      <![CDATA[Flip Manhole cover(s) over, obviously past vertical *without it/them ever reaching Base.]]></string>
        <string id="M08-scoring1">                  Manhole cover(s) that are flipped over past vertical (and never reached Base)</string>
        <string id="M08-scoring2">                  2 in separate Tripod targets</string>
        <string id="M08-scoring3">                  Both covers are flipped over and completely in separate Tripod targets</string>
        <string id="M08-error">                     Impossible combination</string>
        
        <string id="M09-name">                      M09 Tripod</string>
        <string id="M09-desc">                      <![CDATA[Move the inspection camera Tripod.]]></string>
        <string id="M09-scoring1">                  All the Tripod’s feet are touching the mat</string>
        <string id="M09-scoring2">                  Tripod is partially in a Tripod target</string>
        <string id="M09-scoring3">                  OR Tripod is completely in a Tripod target</string>
        <string id="M09-scoring4">                  All the Tripod’s feet are touching the mat and Tripod is in a Tripod target</string>
        <string id="M09-scoring5">                  No</string>
        <string id="M09-scoring6">                  Partially</string>
        <string id="M09-scoring7">                  Completely</string>
        <string id="M09-error">                     Choose one of the two options</string>
        
        <string id="M10-name">                      M10 Pipe Replacement</string>
        <string id="M10-desc">                      <![CDATA[Move a New Pipe so it is where the broken one started, in full/flat contact with the mat.]]></string>
        <string id="M10-scoring1">                  New Pipe is installed where Broken Pipe was</string>
        <string id="M10-scoring2">                  This New Pipe has full/flat contact with the mat</string>
        <string id="M10-scoring3">                  New Pipe is installed where Broken Pipe was, in full/flat contact with the mat</string>
        
        <string id="M11-name">                      M11 Pipe Construction</string>
        <string id="M11-desc">                      <![CDATA[Move a New Pipe.]]></string>
        <string id="M11-scoring1">                  New Pipe has full/flat contact with the mat</string>
        <string id="M11-scoring2">                  This New Pipe is partially in its target</string>
        <string id="M11-scoring3">                  OR this New Pipe is completely in its target</string>
        <string id="M11-scoring4">                  New Pipe has full/flat contact with the mat and is in its target</string>
        <string id="M11-scoring5">                  No</string>
        <string id="M11-scoring6">                  Partially</string>
        <string id="M11-scoring7">                  Completely</string>
        
        <string id="M12-name">                      M12 Sludge</string>
        <string id="M12-desc">                      <![CDATA[Move the Sludge so it is touching the visible wood of any of the six drawn garden boxes.]]></string>
        <string id="M12-scoring">                   Sludge is touching the visible wood of a drawn garden box</string>
                           
        <string id="M13-name">                      M13 Flower</string>
        <string id="M13-desc">                      <![CDATA[Make the Flower rise some obvious height and stay there, due only to a Big Water in the brown pot.]]></string>
        <string id="M13-scoring1">                  Flower is raised (due only to a Big Water in brown pot)</string>
        <string id="M13-scoring2">                  At least one rain is in the purple part, touching nothing but Flower model</string>

        <string id="M14-name">                      M14 Water Well</string>
        <string id="M14-desc">                      <![CDATA[Move the Water Well so it has contact with the mat.]]></string>
        <string id="M14-scoring1">                  Water Well has contact with the mat partially inside the target area</string>
        <string id="M14-scoring2">                  Water Well has contact with the mat completely inside the target area</string>
        <string id="M14-scoring4">                  Water Well has contact with the mat inside the target area</string>
        <string id="M14-scoring5">                  No</string>
        <string id="M14-scoring6">                  Partially</string>
        <string id="M14-scoring7">                  Completely</string>
        
        <string id="M15-name">                      M15 Fire</string>
        <string id="M15-desc">                      <![CDATA[ake the fire drop *only by making the Firetruck apply direct force to the House’s lever.]]></string>
        <string id="M15-scoring">                   Fire is dropped (due only to Firetruck applying direct force to House’s lever)</string>
        
        <string id="M16-name">                      M16 Water Collection</string>
        <string id="M16-desc">                      <![CDATA[Move or catch Big Water and/or Rain water (one Rain maximum; no Dirty Water) so it is touching the mat in the Water Target, *without the target ever reaching the white Off-Limits Line shown below.  Water may be touching the target, and/or other water, but not be touching nor guided by anything else.  Each water model is scored as an individual.]]></string>
        <string id="M16-scoring1">                  Water Target is East of Off-Limits line (and never reached Off-Limit line)</string>
        <string id="M16-scoring2">                  At least one Rain is touching mat in Water Target</string>
        <string id="M16-scoring3">                  Big Water touching mat in Water Target</string>
        <string id="M16-scoring4">                  At least one pair of Big Waters are stacked in Water Target</string>
        
        <string id="M17-name">                      M17 Slingshot</string>
        <string id="M17-desc">                      <![CDATA[Move the Slingshot so it is completely in its target.]]></string>
        <string id="M17-scoring1">                  Slingshot is completely in the Slingshot target</string>
        <string id="M17-scoring2">                  Rain AND Dirty Water are completely in the Slingshot target</string>
        
        <string id="M18-name">                      M18 Faucet</string>
        <string id="M18-desc">                      <![CDATA[Make the water level obviously more blue than white as seen from above the cup, *only by turning the Faucet handle.]]></string>
        <string id="M18-scoring">                   Water level is more blue than white (only by turning Faucet handle)</string>
        
        <string id="penalties-name">                Penalties</string>
        <string id="penalties-desc">                <![CDATA[Penalties given]]></string>
        <string id="penalties-scoring">             Number of Penalty discs in the white triangle area</string>
                             
    </strings>

