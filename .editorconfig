# fllscoring

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

# Unix-style newlines with a newline ending every file
[src/*]
indent_style = space
indent_size = 4

[*.js]
indent_style = space
indent_size = 4

[*.xml]
indent_style = space
indent_size = 4

[*.xsd]
indent_style = space
indent_size = 4
